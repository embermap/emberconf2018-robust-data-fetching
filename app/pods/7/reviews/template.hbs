<div class="mt-1">
  {{#link-to "7.index"
    class="text-5 text-grey-dark no-underline"}}
    Back to stock list
  {{/link-to}}
</div>

<h1>
  {{model.symbol}} reviews
</h1>

<ul class="list-reset p-0 mr-12" {{highlight-changes}}>
  {{#each model.reviews as |review|}}
    <li class="mb-1">
      {{review.rating}} / 5 stars
    </li>
  {{/each}}
</ul>
